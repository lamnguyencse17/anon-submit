{
  "name": "anon-submit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node -r dotenv/config .env && ./node_modules/.bin/next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-migration": "bash ./scripts/create_migration_file.sh",
    "migrate": "swc src/database/ -d dist --config-file .swcrc.migrate && node dist/database/migrate.js && rm -rf dist",
    "gen-kysely": "kysely-codegen --dialect postgres --out-file src/database/database.d.ts",
    "prepare": "husky install",
    "format": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "postinstall": "pnpm migrate && pnpm gen-kysely",
    "extract": "lingui extract --clean",
    "compile": "lingui compile"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.2.0",
    "@types/node": "20.4.10",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^9.0.0",
    "dayjs": "^1.11.9",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "immer": "^10.0.2",
    "jose": "^4.14.4",
    "kysely": "^0.26.1",
    "next": "13.4.19",
    "pg": "^8.11.2",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.76",
    "@types/bcryptjs": "^2.4.2",
    "@types/pg": "^8.10.2",
    "dotenv": "^16.3.1",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "kysely-codegen": "^0.10.1",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "type-fest": "^4.2.0"
  }
}
